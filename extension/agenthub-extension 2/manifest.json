{
  "manifest_version": 3,
  "name": "AgentHub Web Extractor",
  "description": "Select and extract text from any page, structure it, and sync securely to AgentHub (Supabase).",
  "version": "0.1.0",
  "action": {
    "default_popup": "src/popup/index.html",
    "default_title": "AgentHub Extractor"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "tabs",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "options_page": "src/options/index.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": []
}


